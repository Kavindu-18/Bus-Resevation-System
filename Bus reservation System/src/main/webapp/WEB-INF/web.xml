<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
 
 
 	<servlet>
		<servlet-name>insert</servlet-name>
		<servlet-class>com.create.insertBusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insert</servlet-name>
		<url-pattern>/Add-New-Bus</url-pattern>
	</servlet-mapping>
	
	 <servlet>
		<servlet-name>UpdateBus</servlet-name>
		<servlet-class>com.create.updateBusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateBus</servlet-name>
		<url-pattern>/Update-Bus</url-pattern>
	</servlet-mapping>
 
	 <servlet>
	    <servlet-name>DeleteBusServlet</servlet-name>
	    <servlet-class>com.create.deleteBusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>DeleteBusServlet</servlet-name>
	    <url-pattern>/delete-Bus</url-pattern>
	</servlet-mapping>
 
 
 
         
</web-app>